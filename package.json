{
  "name": "markdown-everything",
  "displayName": "Markdown Everything",
  "description": "Your life need to be markdowned.",
  "keywords": [
    "markdown",
    "footnotes",
    "md",
    "documentation",
    "pdf"
  ],
  "publisher": "1chooo",
  "icon": "docs/logo.png",
  "version": "0.0.1",
  "license": "Apache",
  "repository": {
    "url": "https://github.com/1chooo/markdown-everything"
  },
  "bugs": {
    "url": "https://github.com/1chooo/markdown-everything/issues"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "markdown.markdownItPlugins": true,
    "commands": [
      {
        "command": "markdown-everything.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "markdown-everything.sayHello",
        "title": "Hello World 2"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "markdown-it-footnote": "^4.0.0"
  }
}
